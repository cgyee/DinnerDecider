  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-build": "cd client && npm run dev-build",
    "build": "npm run build",
    "heroku-postBuild": " cd client && npm run dev-build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "debug-server": "nodemon --inspect server.js",
    "debug-server-brk": "nodemon --inspect-brk server.js",
    "debug": "concurrently --kill-others-on-fail \"npm run debug-server\" \"npm run client\"",
    "debug-brk": "concurrently --kill-others-on-fail \"npm run debug-server-brk\" \"npm run client\"",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
